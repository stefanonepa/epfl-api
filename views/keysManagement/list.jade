extends ../layout

block content
   
    if keys.length > 0
        div(style='width: 450px;')
            table(class="table table-striped table-hover table-condensed" )
                thead
                    tr
                        th Keys
                        th Remove
                tbody
                    each key in keys
                        tr
                            td
                                !{key}
                            td
                                a(href='/app/keysManagement/delete/#{key}' class='btn  btn-danger') &times;
     a(href="/app/keysManagement/add" class='btn  btn-success') ADD
                
                